{
    "manifest_version": 3,
    "name": "LinkedIn Easy Apply Job Fetcher",
    "version": "1.1",
    "description": "Fetch Easy Apply LinkedIn job IDs.",
    "permissions": ["scripting", "activeTab", "downloads"],
    "host_permissions": ["https://www.linkedin.com/*"],
  
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
  
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
  
    "background": {
      "service_worker": "background.js"
    }
  }
  